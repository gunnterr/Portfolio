{
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "parser": "babel-ts",
        "semi": true,
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ]
  }
}